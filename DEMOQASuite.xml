<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener
			class-name="com.cctech.challenge.utils.ExtentReportManager"></listener>
	</listeners>
	<test thread-count="5" name="chromeTest">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.cctech.challenge.tests.TC001_Submit_form_with_valid_data" />
			<class
				name="com.cctech.challenge.tests.TC002_Submit_form_with_multiple_subjects_and_hobbies" />

			<class
				name="com.cctech.challenge.tests.TC003_Submit_form_with_all_fields_empty" />

			<class
				name="com.cctech.challenge.tests.TC004_Submit_form_with_invalid_email_format" />

			<class
				name="com.cctech.challenge.tests.TC005_Submit_form_with_invalid_mobile_number_less_than_10_digits" />
		</classes>
	</test> <!-- Test -->



</suite> <!-- Suite -->
